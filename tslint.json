{
    "rulesDirectory": ["tslint-plugin-prettier"],
    "extends": ["tslint:latest", "tslint-react", "tslint-config-prettier"],
    "rules": {
        "prettier": true,
        "jsx-no-lambda": false,
        "import-name": false,
        "jsx-no-multiline-js": false,
        "no-boolean-literal-compare": false,
        "quotemark": [true, "single", "jsx-single", "avoid-escape"]
    }
}